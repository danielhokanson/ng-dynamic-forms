{
  "name": "@danielhokanson/ng-dynamic-forms",
  "version": "21.0.0",
  "description": "A rapid form development library for Angular",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danielhokanson/ng-dynamic-forms.git"
  },
  "author": "Udo Sch√∂fer <ng2-dynamic-forms@udos86.de> (https://github.com/udos86/ng-dynamic-forms) - forked by Daniel Hokanson <nu-nya@business.com> (https://github.com/danielhokanson/ng-dynamic-forms)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/danielhokanson/ng-dynamic-forms/issues"
  },
  "homepage": "https://github.com/udos86/ng-dynamic-forms#readme",
  "schematics": "./schematics/collection.json",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "npm run test:app && npm run test:lib",
    "test:app": "npx ng test ng-dynamic-forms --code-coverage",
    "lint": "ng lint",
    "build:sample": "npx ng build ng-dynamic-forms -c production --base-href http://ng2-dynamic-forms.udos86.de/sample/",
    "build:lib": "npm run build:core && npm run build:ui-basic && npm run build:ui-bootstrap && npm run build:ui-foundation && npm run build:ui-ionic && npm run build:ui-material && npm run build:ui-ng-bootstrap && npm run build:ui-ngx-bootstrap && npm run build:ui-primeng",
    "build:core": "npx ng build @danielhokanson/ng-dynamic-forms-core -c production",
    "build:ui-basic": "npx ng build @danielhokanson/ng-dynamic-forms-ui-basic -c production",
    "build:ui-bootstrap": "npx ng build @danielhokanson/ng-dynamic-forms-ui-bootstrap -c production",
    "build:ui-foundation": "npx ng build @danielhokanson/ng-dynamic-forms-ui-foundation -c production",
    "build:ui-ionic": "npx ng build @danielhokanson/ng-dynamic-forms-ui-ionic -c production",
    "build:ui-material": "npx ng build @danielhokanson/ng-dynamic-forms-ui-material -c production",
    "build:ui-ng-bootstrap": "npx ng build @danielhokanson/ng-dynamic-forms-ui-ng-bootstrap -c production",
    "build:ui-ngx-bootstrap": "npx ng build @danielhokanson/ng-dynamic-forms-ui-ngx-bootstrap -c production",
    "build:ui-primeng": "npx ng build @danielhokanson/ng-dynamic-forms-ui-primeng -c production",
    "build:schematics": "tsc -p ./schematics/tsconfig.json",
    "doc:lib": "compodoc -p projects/tsconfig.compodoc.json -d ./docs",
    "test:lib": "npm run test:core && npm run test:ui-basic && npm run test:ui-bootstrap && npm run test:ui-foundation && npm run test:ui-ionic && npm run test:ui-material && npm run test:ui-ng-bootstrap && npm run test:ui-ngx-bootstrap && npm run test:ui-primeng",
    "test:core": "npx ng test @danielhokanson/ng-dynamic-forms-core --code-coverage",
    "test:ui-basic": "npx ng test @danielhokanson/ng-dynamic-forms-ui-basic --code-coverage",
    "test:ui-bootstrap": "npx ng test @danielhokanson/ng-dynamic-forms-ui-bootstrap --code-coverage",
    "test:ui-foundation": "npx ng test @danielhokanson/ng-dynamic-forms-ui-foundation --code-coverage",
    "test:ui-ionic": "npx ng test @danielhokanson/ng-dynamic-forms-ui-ionic --code-coverage",
    "test:ui-material": "npx ng test @danielhokanson/ng-dynamic-forms-ui-material --code-coverage",
    "test:ui-ng-bootstrap": "npx ng test @danielhokanson/ng-dynamic-forms-ui-ng-bootstrap --code-coverage",
    "test:ui-ngx-bootstrap": "npx ng test @danielhokanson/ng-dynamic-forms-ui-ngx-bootstrap --code-coverage",
    "test:ui-primeng": "npx ng test @danielhokanson/ng-dynamic-forms-ui-primeng --code-coverage",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "size-check": "npm run build:lib && size-limit",
    "update:ng": "ng update @angular/cli @angular/core @angular/cdk @angular/material @angular-devkit/schematics @angular-devkit/schematics-cli --force",
    "update:version": "schematics .:versioning --debug=false",
    "publish": "./publish.sh"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^21.0.0",
    "@angular/cdk": "^21.0.0",
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/localize": "^21.0.0",
    "@angular/material": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/platform-browser-dynamic": "^21.0.0",
    "@angular/router": "^21.0.0",
    "@ionic/angular": "^8.7.10",
    "@ng-bootstrap/ng-bootstrap": "^19.0.1",
    "@primeuix/utils": "^0.6.3",
    "bootstrap": "^5.3.8",
    "core-js": "^3.39.0",
    "foundation-sites": "^6.9.0",
    "ngx-bootstrap": "^20.0.2",
    "ngx-mask": "^20.0.3",
    "primeicons": "^7.0.0",
    "primeng": "^20.3.0",
    "quill": "^2.0.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "~0.15.0"
  },
  "overrides": {
    "form-data": "^4.0.1",
    "tough-cookie": "^4.1.4",
    "xml2js": "^0.6.2",
    "tmp": "^0.2.3",
    "@ng-bootstrap/ng-bootstrap": {
      "@angular/common": "^21.0.0",
      "@angular/core": "^21.0.0",
      "@angular/forms": "^21.0.0",
      "@angular/localize": "^21.0.0"
    },
    "ngx-bootstrap": {
      "@angular/common": "^21.0.0",
      "@angular/core": "^21.0.0",
      "@angular/forms": "^21.0.0",
      "@angular/animations": "^21.0.0"
    },
    "primeng": {
      "@angular/common": "^21.0.0",
      "@angular/core": "^21.0.0",
      "@angular/forms": "^21.0.0",
      "@angular/animations": "^21.0.0",
      "@angular/cdk": "^21.0.0",
      "@angular/platform-browser": "^21.0.0",
      "@angular/platform-browser-dynamic": "^21.0.0",
      "@angular/router": "^21.0.0"
    }
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^21.0.0",
    "@angular-devkit/core": "^21.0.0",
    "@angular-devkit/schematics": "^21.0.0",
    "@angular-devkit/schematics-cli": "^21.0.0",
    "@angular-eslint/builder": "^20.6.0",
    "@angular-eslint/eslint-plugin": "^20.6.0",
    "@angular-eslint/eslint-plugin-template": "^20.6.0",
    "@angular-eslint/schematics": "^20.6.0",
    "@angular-eslint/template-parser": "^20.6.0",
    "@angular/cli": "^21.0.0",
    "@angular/compiler-cli": "^21.0.0",
    "@angular/language-service": "^21.0.0",
    "@compodoc/compodoc": "^1.1.32",
    "@playwright/test": "^1.56.1",
    "@popperjs/core": "^2.11.8",
    "@types/jasmine": "~5.1.4",
    "@types/jasminewd2": "^2.0.13",
    "@types/node": "^22.9.0",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "eslint": "^9.39.1",
    "jasmine-core": "~5.4.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "ng-packagr": "^21.0.0",
    "size-limit": "^11.2.0",
    "ts-node": "~10.9.2",
    "typescript": "~5.9.3"
  },
  "main": "index.js"
}
